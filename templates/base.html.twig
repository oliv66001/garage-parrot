		<link href="{{ asset('assets/css/Hover-master/css/hover-min.css') }}" rel="stylesheet" media="all">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		<link
		rel="stylesheet" href="{{ asset('assets/css/main.css') }}">


		<!-- Fichiers JS -->
		<script src="{{ asset('assets/js/jQuery3.6.3.js') }}"></script>
		{#<script src="{{ asset('assets/js/jQuery1.12.4.js') }}"></script>#}
