		<script src="https://kit.fontawesome.com/dee9b678cd.js" crossorigin="anonymous"></script>
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		
		<script src="{{ asset('assets/js/scripts.js') }}" defer></script>
		


	</head>
	<body>
		{% include "_partials/_nav.html.twig" %}
